    "express-session": "^1.17.3",
    "file-type": "^18.5.0",
    "handlebars": "^4.7.7",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.1.13",
    "resend": "^0.17.2",
    "rimraf": "^5.0.1",
    "rxjs": "^7.2.0",
    "socket.io": "^4.6.2"
